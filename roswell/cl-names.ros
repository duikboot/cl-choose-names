#!/bin/sh
#|-*- mode:lisp -*-|#
#| Choose name
exec ros -Q -- $0 "$@"
|#



(unless (find-package :uiop)
  (ql:quickload :uiop :silent t))

(ql:quickload :names :silent t)

(defun print-usage ()
 (princ
  "Names
<number> is the number of names to process at a time.

Usage:
    $ names <number>
"))

(defun main (&optional n &rest argv)
  (declare (ignore argv))
  (when n
    (names:start (parse-integer n))
    (uiop:quit 0))
  (print-usage)
  (uiop:quit 1))


